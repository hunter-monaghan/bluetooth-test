{
  "name": "bluetooth-test",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "setup-scripting-for-windows": "npm config set script-shell ${HOME}/AppData/Local/Programs/Git/usr/bin/sh",
    "start": "node ./DevTools/tunnelTimeoutFixer.js && APP_VARIANT=development expo start --tunnel",
    "start:prod": "node ./DevTools/tunnelTimeoutFixer.js && APP_VARIANT=production expo start --tunnel",
    "start:local": "APP_VARIANT=development expo start",
    "android": "APP_VARIANT=development expo start --android",
    "ios": "APP_VARIANT=development expo start --ios",
    "web": "APP_VARIANT=development expo start --web",
    "test": "APP_VARIANT=development jest",
    "test:watch": "APP_VARIANT=development jest --watch --coverage=false --changedSince=origin/main",
    "test:debug": "APP_VARIANT=development jest -o --watch --coverage=false",
    "test:final": "APP_VARIANT=development jest",
    "updateSnapshots": "APP_VARIANT=development jest -u --coverage=false",
    "lint": "eslint --ext .js --ext .jsx App.js",
    "lint:fix": "eslint --ext .js --ext .jsx App.js --fix",
    "code-cleanup": "npm run prettify && npm run lint && npm run test",
    "prepare": "husky install",
    "prettify": "prettier --write App.js",
    "register-apple-device-for-preview": "eas device:create",
    "push-metadata": "eas metadata:push",
    "build:dev": "eas build --profile development --platform all --non-interactive",
    "build:preprod": "eas build --profile preprod --platform all --non-interactive",
    "build:staging": "eas build --profile staging --platform all --non-interactive",
    "build:qa": "eas build --profile qa --platform all --non-interactive",
    "build:preview": "eas build --profile preview --platform all --non-interactive",
    "build:prod": "eas build --profile production --platform all --non-interactive",
    "build:dev:android": "eas build --profile development --platform android --non-interactive",
    "build:preview:android": "eas build --profile preview --platform android --non-interactive",
    "build:prod:android": "eas build --profile production --platform android --non-interactive",
    "build:staging:android": "eas build --profile staging --platform android --non-interactive",
    "build:qa:android": "eas build --profile qa --platform android --non-interactive",
    "build:dev:ios": "eas build --profile development --platform ios --non-interactive",
    "build:preview:ios": "eas build --profile preview --platform ios --non-interactive",
    "build:prod:ios": "eas build --profile production --platform ios --non-interactive",
    "build:staging:ios": "eas build --profile staging --platform ios --non-interactive",
    "build:qa:ios": "eas build --profile qa --platform ios --non-interactive",
    "release": "npm version patch --sign-git-tag true --commit-hooks false && git push --tags && node updateAndroidVersion",
    "release:minor": "npm version minor --sign-git-tag true --commit-hooks false && git push --tags && node updateAndroidVersion",
    "release:major": "npm version major --sign-git-tag true --commit-hooks false && git push --tags && node updateAndroidVersion"
  },
  "resolutions": {
    "react-native-svg": "13.4.0"
  },
  "dependencies": {
    "@expo/config-plugins": "~6.0.0",
    "@react-native-firebase/analytics": "^18.6.0",
    "@react-native-firebase/app": "^18.6.0",
    "@react-native-firebase/crashlytics": "^18.6.0",
    "@react-native-firebase/perf": "^18.6.0",
    "@react-navigation/material-top-tabs": "^6.6.3",
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/native-stack": "^6.9.14",
    "@react-navigation/stack": "^6.3.18",
    "cross-env": "^7.0.3",
    "expo": "^49.0.6",
    "expo-auth-session": "~5.0.2",
    "expo-build-properties": "~0.8.3",
    "expo-checkbox": "~2.4.0",
    "expo-constants": "~14.2.1",
    "expo-dev-client": "~2.4.6",
    "expo-device": "~5.4.0",
    "expo-font": "~11.4.0",
    "expo-image": "~1.3.2",
    "expo-linking": "~5.0.2",
    "expo-local-authentication": "~13.4.1",
    "expo-secure-store": "~12.3.1",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-store-review": "~6.4.0",
    "expo-tracking-transparency": "~3.1.0",
    "expo-updates": "~0.18.11",
    "expo-web-browser": "~12.3.2",
    "jest": "^29.2.1",
    "jest-expo": "^48.0.2",
    "lodash": "^4.17.21",
    "lottie-react-native": "^6.3.1",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-hook-form": "^7.45.1",
    "react-native": "0.72.3",
    "react-native-ble-plx": "^3.1.2-rc.0",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-pager-view": "6.2.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-webview": "^13.6.0"
  },
  "devDependencies": {
    "@testing-library/react-native": "^12.3.0",
    "babel-jest": "^29.5.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.37.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-react-native-a11y": "^3.3.0",
    "expo-constants": "~14.4.2",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "react-test-renderer": "^18.2.0",
    "tailwindcss": "3.3.2",
    "uri-scheme": "^1.1.0"
  },
  "private": true
}
